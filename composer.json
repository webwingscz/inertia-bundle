{
  "name": "webwingscz/inertia-bundle",
  "description": "Server-side adapter for Symfony 7 and Inertia.js v2",
  "homepage": "https://github.com/webwingscz/inertia-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Webwings\\InertiaBundle\\": "src/"
    }
  },
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/http-client": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/security-csrf": "^7.0",
    "symfony/serializer": "^7.0",
    "symfony/twig-bundle": "^7.0",
    "twig/twig": "^2.12|^3.0",
    "illuminate/collections": "^11.46|^12.34"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-symfony": "^2.0",
    "symfony/framework-bundle": "^7.0",
    "symfony/var-dumper": "^7.0",
    "symfony/yaml": "^7.0",
    "doctrine/orm": "^3.3"
  },
  "scripts": {
    "fix": [
      "@fix:phpcs",
      "@fix:phpstan"
    ],
    "fix:phpstan": [
      "phpstan analyse -c phpstan.neon --memory-limit=8G"
    ],
    "fix:phpcs": [
      "php-cs-fixer fix"
    ]
  }
}
